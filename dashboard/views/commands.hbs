{{>navbar}}


<div id="body" class="container mt-5">
    <div class="table-responsive">
        <table class="table table-striped table-dark text-white table-hover">
            <thead>
                <h3 class="cmdheading">Moderation üõ°Ô∏è</h3>
                <tr>
                    <th colspan="2">Command</th>
                    <th>Usage</th>
                    <th>Description</th>
                    <th>Type</th>
                    <th>Permissions</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                {{#each modcmds}}
                <tr>
                    <td colspan="2">
                        <h6> {{this.command}} </h6>
                    </td>
                    <td>
                        <div class="d-flex align-items-center"><span class="ml-2"> {{this.usage}} </span>
                        </div>
                    </td>
                    <td> {{this.description}}<br></td>
                    <td class="font-weight-bold"> {{this.type}} </td>
                    <td> {{this.perms}} </td>
                    <td><i class="fa fa-external-link external-link"></i></td>
                </tr>
                {{/each}}

            </tbody>
        </table>
    </div>
</div>

